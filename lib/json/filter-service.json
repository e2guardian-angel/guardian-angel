{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "webfilter"
  },
  "spec": {
    "selector": {
      "app": "webfilter"
    },
    "ports": [
      {
        "name": "squid",
        "protocol": "TCP",
        "port": 3128,
        "targetPort": 3128
      },
      {
        "name": "transocks",
        "protocol": "TCP",
        "port": 12345,
        "targetPort": 12345
      }
    ]
  }
}